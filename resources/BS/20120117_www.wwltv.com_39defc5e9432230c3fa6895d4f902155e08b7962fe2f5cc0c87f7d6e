Cookie();	 						} else if (commentCookieCheck == "oldestToNewest") { 							$('#commentOrder').val("oldestToNewest"); 							 this.api.showComments(); 							 this.api.reverseOrder(); 							 jQuery("#comments-reorder").slideDown(); 						}else if (commentCookieCheck == "newestToOldest") { 							$('#commentOrder').val("newestToOldest"); 							 this.api.showFifteen(); 							 this.api.showComments(); 							 jQuery("#comments-reorder").slideDown(); 						}else if (commentCookieCheck == "None") { 							$('#commentOrder').val("None"); 							 this.api.hideComments();	  						}; 						jQuery("#commentOrder option select").val(commentCookieCheck).attr('selected', 'selected'); 						return this.api; 					} 			} 			 			//initiate Comment reordering tools            	var commentRun = commentsDisplay.init(); 			$('#commentOrder').bind('change',function() {  				previousOrder = commentRun.readCookie("beloCommentOrder"); 				commentRun.commentOrderCheck(previousOrder);	 			});  			 										//Function that configures the modal window options 		$('.comment div.commentFlag').click(function(e) { 			var abusiveComment = $(e.target).prev().text(); 			var commentorName = $(e.target).siblings('p.comment-details'); 			var commentorName = $(commentorName).children('.comment-author'); 			var commentorName = $(commentorName).text(); 			var abusiveCommenttext = $(e.target).siblings('p.comment-body').get(0); 			var abusiveCommenttext = $(abusiveCommenttext).text(); 			var abusiveCommenttext = '\"' + abusiveCommenttext + '\"'; 			 			$('#comment-flag-reason').focus(); 			$('#other-field-container').slideUp().addClass('hide');	 			$('#flag-submit').attr("disabled","disabled"); 			$('#comment-flag-proc').removeClass('hide'); 			$('#comment-flag-succ').addClass('hide'); 			$('#comment-flag-reason').val("Select One"); 			$('#otherfield').val(''); 			$('.comment-lightbox #flaggedCommentId').val(abusiveComment); 			$('.comment-lightbox #comment-text').html(abusiveCommenttext); 			$('.comment