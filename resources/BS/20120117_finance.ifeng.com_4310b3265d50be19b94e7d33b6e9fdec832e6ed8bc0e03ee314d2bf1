<p> var IfengAdCookie = { 	author:"JingWu", 	version:"0.0.1", 	tmp:{}, 	set:function(param) { 		if(typeof(param) === 'undefined' || typeof(param.name) === 'undefined' || typeof(param.value) === 'undefined') { 			alert('参数有误，请返回修改! ex:{\'name\':,\'value\':,\'time\':}'); 			return false; 		} 		if(typeof(param.time) === 'undefined') { 			var now = new Date(); 			var date = new Date(now.getFullYear(), (now.getMonth() + 1), now.getDate(), 23, 59, 59); 		} else { 			var date = new Date(param.time); 		} 		 		document.cookie = param.name + "=" + escape(param.value) + ";path=/;expires=" + date.toGMTString() + ";" 	}, 	get:function(param) { 		if(typeof(param) === 'undefined' || typeof(param.name) === 'undefined') { 			alert('参数有误，请返回修改! ex:{\'name\':}'); 			return false; 		} 		var c = document.cookie.split("; "); 		for(var i = 0; i < c.length; i ++) { 			var d = c[i].split("="); 			if(d[0] == param.name) { 				return unescape(d[1]); 			} 		} 		return ""; 	} };  var IfengAdAlternate = { 	version:'0.0.1', 	author:'JingWu', 	tmp:{}, 	stock:{}, 	play:function() { 		if(this.checkParam()) { 		} else { 			return false; 		} 		this.init_stock(); 		this.install(); 	}, 	checkParam:function() { 		var param = this.stock[this.tmp.id]; 		var len = this.stock[this.tmp.id].length; 		for(var i=0; i<len; i++) { 			if(typeof(param[i]) == 'undefined' ) { 				continue; 			} 			if( param[i].length == 4 ){ 				if( (param[i][0] === '') ) { 					alert('广告代码参数有误，在第 ' + i + ' 个广告，第 1 个参数'); 					return false; 				} 				if( param[i][1] === '' || isNaN(param[i][1]) ) { 					alert('广告代码参数有误，在第 ' + i + ' 个广告，第 2 个参数'); 					return false; 				} 				if( param[i][2] === true || param[i][2] === false ) { 				} else { 					alert('广告代码参数有误，在第 ' + i + ' 个广告，第 3 个参数'); 					return false; 				} 				if( param[i][3] === '' ) { 					alert('广告代码参数有误，在第 ' + i + ' 个广告，第 4 个参数'); 					return false; 				} 			} else { 				alert('第 ' + i + ' 个广告参数不全'); 				return false; 			} 		} 		return true; 	}, 	checkInstall:function() { 		if(typeof(IfengAdAlternate.tmp.checkInstall) == 'undefined') { 			IfengAdAlternate.tmp.checkInstall = 0; 		} 		document.write('<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/html/birangtest/checkInstall.js\">'+'</'+'script>'); 	}, 	install:function() { 		IfengAdAlternate.tmp.checkInstall = 0; 		var current_ad = this.getSequence(); 		if(current_ad === 'complete') { 			return true; 		} else { 			document.write(current_ad[0]); 			this.tmp.current = current_ad;			//	将当前播放的广告放入当前页面的临时变量 			this.checkInstall(); 		} 	}, 	installSucess:function() { 		return true; 	}, 	installFail:function() { 		if(IfengAdAlternate.stock[IfengAdAlternate.tmp.id].length == 1) { 			return true; 		} 		IfengAdAlternate.tmp.checkInstall = 0; 		IfengAdAlternate.install(); 	}, 	cookie_flag:function() { 		var flag_cookie = ""; 		var domain = document.domain.substr(0, (document.domain.length-10)); 		 		var href = document.location.href; 		var start = href.lastIndexOf("/") + 1; 		var end = href.lastIndexOf("."); 		href = href.substr(start, end-start); 		 		flag_cookie = domain + href; 		return 'IfengAA_' + flag_cookie; 	}, 	getSequence:function() { 		var len_current_level = 0; 		var cookie_arr = new Array(); 		var next_level = 0; 		var level = 0; 		 		IfengAdAlternate.tmp.complete = IfengAdAlternate.tmp.complete + 1; 		//	获取 cookie	 		var cookieFlag = IfengAdAlternate.cookie_flag(); 		var cookie = IfengAdCookie.get({'name': cookieFlag + IfengAdAlternate.tmp.id});  		//	判断需要播放的广告 		if(cookie == '') { 			len_current_level = IfengAdAlternate.tmp.level_list[IfengAdAlternate.tmp.level_arr[0]].length; 			if(len_current_level == 1) {	//	只有一个广告 				cookie_arr = [0, 0, 1]; 			} else { 				cookie_arr = [0, IfengAdAlternate.random(len_current_level), 1]; 			} 		} else { 			cookie_arr = cookie.split('_'); 			level = IfengAdAlternate.tmp.level_arr[cookie_arr[0]]; 			len_current_level = IfengAdAlternate.tmp.level_list[level].length; 			if(cookie_arr[2] >= len_current_level) { 				//	当前级别广告是否播放完成 				if(cookie_arr[0] >= IfengAdAlternate.tmp.level_arr.length-1) { 					//	广告轮播完成，重新开始轮播 					len_first_level = IfengAdAlternate.tmp.level_list[IfengAdAlternate.tmp.level_arr[0]].length; 					if(len_first_level == 1) {	//	只有一个广告 						cookie_arr = [0, 0, 1]; 					} else { 						cookie_arr = [0, IfengAdAlternate.random(len_first_level), 1]; 					} 				} else { 					//	播放下一级别广告 					next_level = IfengAdAlternate.tmp.level_arr[parseInt(cookie_arr[0])+1]; 					len_next_level = IfengAdAlternate.tmp.level_list[next_level].length; 					if(len_next_level == 1) {	//	只有一个广告 						cookie_arr = [parseInt(cookie_arr[0])+1, 0, 1]; 					} else { 						cookie_arr = [parseInt(cookie_arr[0])+1, IfengAdAlternate.random(len_next_level), 1]; 					} 				} 			} else { 				if(cookie_arr[1] >= len_current_level-1) { 					cookie_arr = [cookie_arr[0], 0, parseInt(cookie_arr[2])+1]; 				} else { 					cookie_arr = [cookie_arr[0], parseInt(cookie_arr[1]) + 1, parseInt(cookie_arr[2])+1]; 				} 			} 		} 		IfengAdCookie.set({'name':cookieFlag + IfengAdAlternate.tmp.id, 'value': cookie_arr[0] + '_' + cookie_arr[1] + '_' + cookie_arr[2]}); 		if(IfengAdAlternate.tmp.complete >= 10) { 			return 'complete'; 		} else { 			level = this.tmp.level_arr[cookie_arr[0]]; 			return this.tmp.level_list[level][cookie_arr[1]]; 		} 	}, 	random:function(len) { 		return (parseInt(Math.random()*100000) % len); 	}, 	init_stock:function() { 		var list = this.stock[this.tmp.id]; 		var len = list.length; 		var len_list=0; 		var level=0; 		var level_arr = new Array(); 		var level_list = new Array(); 		 		// 序列存储对原始数据的要求为：按等级顺序排列 		for(var i=0; i<len; i++) { 			level = list[i][1];		//	获取广告级别 			if(level_arr[level_arr.length-1] == level) {	//	广告级别已存在，将广告放入同级别数组中 				len_list = level_list[level].length; 				level_list[level][len_list] = list[i]; 			} else {	//	新的广告级别 				level_arr[level_arr.length] = level; 				level_list[level] = [list[i]]; 			} 		} 		IfengAdAlternate.tmp.level_arr = level_arr; 		IfengAdAlternate.tmp.level_list = level_list; 	}, 	init:function(param) { 		this.tmp.id = param.id; 		this.tmp.complete = 0; 		this.stock[param.id] = param.list; 		this.play(); 	} };  var Alternate_inner={ id:'fashion', list:[ 		['<scr'+'ipt language="javascript" type="text/javascript" src="http://sc.ifeng.com/js.ng/level=content&location=1&site=ifeng&type=ifocus\">'+'</'+'script>',1,true,"_IFOCUS_CONTROL_"], 		['<scr'+'ipt language="javascript" type="text/javascript" src="http://static.acs86.com/ifeng/41385.js\">'+'</'+'script>',2,true,"_ACS86_ENABLE_41385_"], 		['<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/html/birangtest/adsame.js\">'+'</'+'script>',2,true,"_ADSAME_CONTROL_"], 		['<scr'+'ipt language="javascript" type="text/javascript" src="http://img.ifeng.com/tres/recommend/cpro/quantone/2010/10/101030/101030-SmartMedia-finance.js\">'+'</'+'script>',2,true,"_SMARTMEDIA_3327886665615566_CTRL_"] 	] };  IfengAdAlternate.init(Alternate_inner); </p>
