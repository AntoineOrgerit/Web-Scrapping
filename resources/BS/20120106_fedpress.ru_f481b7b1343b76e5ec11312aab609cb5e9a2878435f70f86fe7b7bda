(tag,attrs){ 			var gross = el.up('div').down(tag); 			var kleine = el.down(tag); 			if (!gross || !kleine) return; 			while (attrs.length) { 				var attr = attrs.shift(); 				gross.setAttribute(attr, kleine.readAttribute(attr)); 			} 		} 		if (!el || el.hasClassName('active')) return false; 			make_active(el); 			toggle('img',['src','alt']); 			//toggle('a',['href']); }  Event.observe(window, 'load', function() { 	if ($('box1') && !$('description')) $('box1').observe('mouseover', function(event) { 			highLht(Event.findElement(event,'H1')); 		} 	);             	$$('ul.anim').each( function (s) { 			if (s.empty()) return; 			var first = s.childElements()[0]; 			first.addClassName('active'); 			//first.down('.img-cont').setStyle({height:'auto'});                         s.setAttribute('id','a_' + s.up('.itema_content').id); 			rollovers.set (s.id, new animated(s.id));  			rollovers.set (s.id, new animated(s.id)); 			s.observe('mouseover', function(event) { 				highLht(Event.findElement(event,'li')); 			} ) 			s.up('.content').observe('mouseout', function(event) { 				stopAnim(event,s); 			} ) 		} 	);  } ); </p>
