<p> answer_id = 0;  sending = 0;  expert = 0;  function answer(id) {  	if(sending==0) {  		expert = 0;  		answer_id = id;  		document.getElementById('answer_form').focus();  		document.getElementById('who_comment').innerHTML = ' &mdash; '+document.getElementById('name_'+id).innerHTML;  		document.getElementById('what_add').innerHTML = 'комментарий';  	}  }  function sendComment() { 	sending = 1; 	var req = new JsHttpRequest(); 	 	var blog_id = document.getElementById('topic_id').value; 	var top = answer_id; 	var text = document.getElementById('answer_form').value; 	if(document.getElementById('exp_zagl')) 		var exp_zagl = document.getElementById('exp_zagl').value; 	if(document.getElementById('exp_desc')) 		var exp_desc = document.getElementById('exp_desc').value; 	 	if(document.getElementById('guest_name')) var guest_name = document.getElementById('guest_name').value; 	else var guest_name = ''; 	if(document.getElementById('code')) var code = document.getElementById('code').value; 	else var code = ''; 	if(document.getElementById('coder')) var cod_er = document.getElementById('coder').innerHTML; 	else var cod_er = ''; 	 	if(expert!=1) { 		document.getElementById('answer_'+answer_id).innerHTML = '<img src="/images/ajax-loader3.gif" width="220" height="19" alt="Загрузка..." />'; 	} else { 		if(document.getElementById('exerts_say').innerHTML=='') { 			document.location = '#exerts_say_go'; 			document.getElementById('exerts_say').innerHTML = '<p>&nbsp;</p><img src="/images/ajax-loader3.gif" alt="Загрузка..." />'; 		} else { 			document.location = '#exerts_say_go'; 			document.getElementById('exerts_say_add').innerHTML = '<img src="/images/ajax-loader3.gif" alt="Загрузка..." />'; 		} 	} 	 	req.onreadystatechange = function() { 		if (req.readyState == 4) { 			if (req.responseJS) { 				if(expert==1) { 					if(!document.getElementById('exerts_say_add')) { 						document.getElementById('exerts_say').innerHTML = req.responseJS.ret; 					} else { 						document.getElementById('exerts_say_add').innerHTML = req.responseJS.ret1; 					} 				} else { 					if(req.responseJS.ret!=null) { 						document.getElementById('answer_'+answer_id).innerHTML = req.responseJS.ret; 						document.getElementById('answer_form').value = ''; 					} else { 						document.getElementById('answer_'+answer_id).innerHTML = 'При отправке возникли ошибки...'; 					} 				} 			} 			sending = 0; 		} 	} 	req.caching = false; 	req.open(null, '/ajax/?act=sendComment', true); 	req.send({  		blog_id: blog_id,  		top: top,  		text: text,  		isnews: isnews,  		guest_name: guest_name,  		code: code,  		coder: cod_er,  		expert: expert, 		exp_zagl:exp_zagl, 		exp_desc:exp_desc 	}); }  function rateComment(where,whom,pm) {  	var req = new JsHttpRequest();  	  	req.onreadystatechange = function() {  		if (req.readyState == 4) {  			if (req.responseJS) {  				if(req.responseJS.ret!=null) {  					document.getElementById('rate_comm_'+where).innerHTML = req.responseJS.ret;  					document.getElementById('pm_bloc_'+where).style.display = 'none';  				} else {  					document.getElementById('rate_comm_'+where).innerHTML = 'При отправке возникли ошибки...';  				}  			}  		}  	}  	req.caching = false;  	req.open(null, '/ajax/?act='+pm, true);  	req.send({ whom: whom, where: where });  } </p>
