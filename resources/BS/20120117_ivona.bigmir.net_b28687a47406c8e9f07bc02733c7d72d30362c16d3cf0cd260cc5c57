<p> /** * inspired by Christopher Manciero * http://www.chrismanciero.com/ * * * */  var timerlen = 5; var slideAniLen = 300; var pluginDivsHeight = {};  // plugin v2 class function PluginV2 (id, num) { 	this.id = id; 	this.num = num; 	this.parent_id = 'parent_' + id; }  PluginV2.prototype = { 	GA_account: 'UA-3119939-34', 	num: 0, // real number of the plugin 	id: '',	// id of the div 	parent_id: '',	// id of the parent div 	moving: false,	// true if it's already moving 	dir: null,	// direction of moving 	endLength: 0,	// length of opened div 	startTime: 0,	// start time 	timerID: 0, // timer id 	obj: null, // div to slide 	initialized: false, 	// whether plugins were initialized  	trackLink: function( domain, pluginName ){ 		 		var pageTracker=_gat._getTracker(this.GA_account);     		pageTracker._trackEvent('баян', domain, pluginName ); 		return true; 	}, 	 	// openes plugin 	open: function() { 		if (this.moving) 			return;  		if (document.getElementById(this.id).style.display != "none") 			return;  		this.addClass(this.parent_id, 'gb-informers_project_open');  		this.moving = true; 		this.dir = 'down'; 		this.startSlide(); 	},  	// closes plugin 	close: function() { 		if (this.moving) 			return;  		if (document.getElementById(this.id).style.display == "none") 			return;  		this.removeClass(this.parent_id, 'gb-informers_project_open'); 		document.getElementById(this.id).style.overflow = 'hidden';  		this.moving = true; 		this.dir = 'up'; 		this.startSlide(); 	},  	// toggles plugin 	toggle: function() { 		if (!this.initialized) { 			return; 		}  		if (document.getElementById(this.id).style.display == "none") { 			// div is hidden, so let's open it 			this.open(); 		} 		else { 			// div is visible, so close it 			this.close(); 		} 	},  	isOpened: function() { 		return document.getElementById(this.id).style.display != "none"; 	},  	// starts plugin slide 	startSlide: function() { 		this.obj = document.getElementById(this.id); 		this.endLength = pluginDivsHeight[this.obj.id]; 		this.startTime = (new Date()).getTime();  		if (this.dir == "down") 			this.obj.style.height = "1px";  		this.obj.style.display = "block";  		var self = this;	// closure  		this.timerID = setInterval(function(){self.slideTick()}, timerlen); 	},  	/** 	*  	checks if it's time to end the slide or to keep sliding 	*/ 	slideTick: function() { 		var elapsed = (new Date()).getTime() - this.startTime;  		if (elapsed > slideAniLen) 			this.endSlide(); 		else { 			var d = Math.round(elapsed / slideAniLen * this.endLength); 			if(this.dir == "up") 				d = this.endLength - d;  			obj = this.obj; 			obj.style.height = d + "px"; 		}  		return; 	},  	/*    	*	ends sliding    	*/ 	endSlide: function () { 		clearInterval(this.timerID);  		if (this.dir == "up") 			this.obj.style.display = "none";  		this.obj.style.height = this.endLength + "px";  		delete(this.moving); 		delete(this.timerID); 		delete(this.startTime); 		delete(this.endLength); 		delete(this.obj); 		delete(this.dir);  		return; 	},  	// some utils  	hasClass: function(id, className) { 		var obj = document.getElementById(id); 		return obj.className.indexOf(className) != -1; 	},  	addClass: function(id, className) { 		var obj = document.getElementById(id); 		if (!this.hasClass(id, className)) obj.className += " " + className; 	},  	removeClass: function(id, className) { 		var obj = document.getElementById(id); 		if (this.hasClass(id, className)) { 			var st = obj.className;     		obj.className = st.replace(' gb-informers_project_open', ''); 		} 	},  	getElementCurrentStyle: function(element, CssStyleProperty) { 		if (!element) return false; 		if (element.currentStyle) { 			return element.currentStyle[this.cssToJavascriptStyle(CssStyleProperty)]; 		} 		else if (window.getComputedStyle) { 			var compStyle = window.getComputedStyle(element, ''); 			return compStyle.getPropertyValue(CssStyleProperty); 		} 		else { 			return ''; 		} 	},  	cssToJavascriptStyle: function(CssProperty) { 		var stringArray = CssPrope