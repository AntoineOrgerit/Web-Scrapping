et_element(tmp_prefix_id+"_diff_inner").style.color = color;       innerHTML(tmp_prefix_id+"_diff_inner",number_format(this.set[secsym].diff,4,",",".",true));                    get_element(tmp_prefix_id+"_price_diff_inner").style.color = color;       innerHTML(tmp_prefix_id+"_price_diff_inner",number_format(this.set[secsym].price_diff,2,",",".",true)+"%");                  get_element(tmp_prefix_id+"_apodosi_etous_inner").style.color = color;       innerHTML(tmp_prefix_id+"_apodosi_etous_inner",number_format(this.set[secsym].apodosi_etous,2,",",".",true)+"%");                      get_element(tmp_prefix_id+"_name_inner").style.background = "yellow";       get_element(tmp_prefix_id+"_price_inner").style.background = "yellow";       get_element(tmp_prefix_id+"_diff_inner").style.background = "yellow";       get_element(tmp_prefix_id+"_price_diff_inner").style.background = "yellow";       get_element(tmp_prefix_id+"_apodosi_etous_inner").style.background = "yellow";   	}   } }   function metoxes_moy_show_prosthesi_metoxis(that){   if(innerHTML(this.inner_popup2,this.draw_prosthesi_metoxis())){     var that_id = (that)?that.id:"";     var p = getElementPosition(that_id)     show(this.inner_popup,(p.left-500), (p.top-210));  	     //makeDraggable(get_element(this.inner_popup2));   } }  function metoxes_moy_draw_prosthesi_metoxis(){   var t = "";   t += "<TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";   t += "<TR>";   t += "  <TD class='big bold'>Πρόσθεση Μετοχής</TD>";   t += "</TR>";    t += "<TR>";   t += "  <TD>Εισάγετε μέχρι 15 σύμβολα</TD>";   t += "</TR>";      t += "<TR>";   t += "  <TD>";   t += "  <TABLE border='0' cellpadding='1' cellspacing='1' width='100%'>";   t += "  <TR>";   var j=0;   for(var i in this.set){   	if(j%5==0 && j>0){   	  t += "</tr><tr>";	   	}   	t += "<TD><INPUT type='text' name='"+make_name(this.name)+"_add_secsym_"+j+"_input' value='"+i+"' class='small border' size='9' maxlength='9' readonly></TD>";    	j++;       }   