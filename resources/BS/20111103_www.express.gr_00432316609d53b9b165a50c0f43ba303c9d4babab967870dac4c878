<p> <!-- // // class metoxes_moy // function Metoxes_moy(name,parent){   this.name = name;   this.parent = parent;   this.inner = make_name(this.name)+"_inner";   this.inner2 = make_name(this.name)+"_inner2";    this.inner_popup4 = make_name(this.name)+"_inner_popup4";   this.inner_popup3 = make_name(this.name)+"_inner_popup3";   this.inner_popup2 = make_name(this.name)+"_inner_popup2";   this.inner_popup = make_name(this.name)+"_inner_popup";   this.inner_help2 = make_name(this.name)+"_inner_help2";   this.inner_help = make_name(this.name)+"_inner_help";   this.listenerAttached = false;      this.getting = false;   this.is_shown = false;   this.is_opened = false; //methods   this.init = metoxes_moy_init;   this.load = metoxes_moy_load;   this.cretera_tosend = metoxes_moy_cretera_tosend;   this.after_get = metoxes_moy_after_get;   this.get = metoxes_moy_get;      this.show = metoxes_moy_show;   this.draw = metoxes_moy_draw;   this.draw_inside = metoxes_moy_draw_inside;   this.draw_changes = metoxes_moy_draw_changes;      this.show_prosthesi_metoxis = metoxes_moy_show_prosthesi_metoxis;   this.draw_prosthesi_metoxis = metoxes_moy_draw_prosthesi_metoxis;   this.hide_prosthesi_metoxis = metoxes_moy_hide_prosthesi_metoxis;    this.show_help_prosthesi_metoxis = metoxes_moy_show_help_prosthesi_metoxis;   this.draw_help_prosthesi_metoxis = metoxes_moy_draw_help_prosthesi_metoxis;   this.hide_help_prosthesi_metoxis = metoxes_moy_hide_help_prosthesi_metoxis;     this.show_afairesi_metoxis = metoxes_moy_show_afairesi_metoxis;   this.draw_afairesi_metoxis = metoxes_moy_draw_afairesi_metoxis;   this.hide_afairesi_metoxis = metoxes_moy_hide_afairesi_metoxis;   this.show_help_afairesi_metoxis = metoxes_moy_show_help_afairesi_metoxis;   this.draw_help_afairesi_metoxis = metoxes_moy_draw_help_afairesi_metoxis;   this.hide_help_afairesi_metoxis = metoxes_moy_hide_help_afairesi_metoxis;   this.rem = metoxes_moy_rem; //   this.init(); }  function metoxes_moy_init(){   this.set = new Array();   this.count = 0;   this.set_just_changed = new Array();   this.last_modified = 0;   this.last_transaction_id = -1;      if (!this.listenerAttached) {     this.listenerAttached = true;     my_objects[my_objects.length] = this;   }	   	 }  function metoxes_moy_load(what){   this.set_just_changed = new Array();   var length = "";var length2 = "";var metavoli=0.0;   for(var i in what){      //var length = this.set.length;     length = what[i]["secsym"];     metavoli = (typeof(this.set[length])=="object" && this.set[length].price>0.0)?(parseFloat(what[i]["price"])-this.set[length].price):0.0; // se sxesi me proigoymeni timi          if(typeof(this.set[length])!="object"){       this.set[length] = new Object();     }     this.set[length].secsym = what[i]["secsym"];     this.set[length].price = parseFloat(what[i]["price"]);     this.set[length].diff = parseFloat(what[i]["diff"]);     this.set[length].price_diff = parseFloat(what[i]["price_diff"]);     this.set[length].apodosi_etous = parseFloat(what[i]["apodosi_etous"]);     this.set[length].metavoli = metavoli;         length2 = this.set_just_changed.length;     this.set_just_changed[length2] = length;   }     delete length; delete length2; }  function metoxes_moy_cretera_tosend(check_cookie){   var toret = new Array();   toret["last_modified"] = this.last_modified;   toret["last_transaction_id"] = this.last_transaction_id;   var secsyms_to_send = new Array();   this.count = 0;   for(var i in this.set){     secsyms_to_send[secsyms_to_send.length] = "'"+this.set[i].secsym+"'";     this.count++;	   }  // alert(d(readCookie("my_secsyms")))   if(this.count>0){     toret["secsyms"] = e(secsyms_to_send.join(","));   }   else if(check_cookie) {   	this.count++;   	toret["secsyms"] = e(d(readCookie("my_secsyms")));   }   var date = new Date();   date.setTime(date.getTime()+(10*365*24*60*60*1000)); //  document.cookie="my_secsyms="+e(secsyms_to_send.join(","))+"; expires="+date.toGMTString()+"; path=/";   document.cookie="my_secsyms="+toret["secsyms"]+"; expires="+date.toGMTString()+"; path=/";   delete secsyms_to_send;   delete date;   return toret; } function metoxes_moy_after_get() {//alert("Dummy metoxes_moy_after_get"); } function metoxes_moy_get(check_cookie){    	   var a = new PHP_Serializer();   var b = a.serialize(this.cretera_tosend(check_cookie));	   if(!this.getting && this.count>0){     var what_to_do = new Object();     what_to_do.callback = do_metoxes_moy_get;     what_to_do.extra_data = this;     x_get_metoxes_moy(b,true,what_to_do);     this.getting = true;   }   else {   	this.is_shown = false;   	this.is_opened = false;   	this.show();   }   delete a; delete b; } function do_metoxes_moy_get(that,aaa){   aaa.getting = false;   var b = that;   if(b["status"]==1){     aaa.load(b["data"]["set"]);     aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;     aaa.last_transaction_id = (aaa.last_transaction_id<b["data"]["last_transaction_id"])?b["data"]["last_transaction_id"]:aaa.last_transaction_id;     aaa.show();   }   else if(b["status"]==0){   	aaa.last_modified = (aaa.last_modified<b["data"]["last_modified"])?b["data"]["last_modified"]:aaa.last_modified;     aaa.show();   }   aaa.after_get();   handle_status(b["status"],b["mesg"]);    delete b; delete that; }  function metoxes_moy_show(){    if(this.is_shown && this.count>0){   	this.draw_changes();   }   else {     if(innerHTML(this.inner,this.draw())){       show(this.inner);     }     if(this.count>0){         if(innerHTML(this.inner2,this.draw_inside())){         if(this.is_opened){           show(this.inner2);         }         else {           hide(this.inner2)		         }       }	     }     else {       hide(this.inner2)	     }   } }  function metoxes_moy_draw(){   var t = "";   t += "<TABLE border='0' cellpadding='0' cellspacing='0' width='100%'>";   t += "<TR>";   t += "  <TD>";   t += "  <TABLE border='0' cellpadding='0' cellspacing='0' width='10