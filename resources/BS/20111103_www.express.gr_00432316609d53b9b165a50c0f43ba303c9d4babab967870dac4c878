toxes_moy_draw_afairesi_metoxis(){   var t = "";   t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='300' class='' >";   t += "<TR>";   t += "  <TD class='big bold'>Αφαίρεση Μετοχής</TD>";   t += "</TR>";    t += "<TR>";   t += "  <TD>Πατήστε πάνω στο «x» δίπλα στο σύμβολο για το αφαιρέσετε απο το χαρτοφυλακίο σας</TD>";   t += "</TR>";      t += "<TR>";   t += "  <TD>";   t += "  <TABLE border='0' cellpadding='2' cellspacing='2'>";   t += "  <TR>";   var j=0;   for(var i in this.set){   	if(j%5==0 && j>0){   	  t += "</tr><tr>";	   	}   	t += "<TD class='small'>"+i+"</td><td><div style='width: 12px; height: 12px;' class='border link bold center bg_white' onclick=\""+this.name+".rem('"+i+"',"+j+");\">X</div></TD>";    	j++;       }   this.count = j;   t += "  </TR>";   t += "  </TABLE>";   t += "  </TD>";   t += "</TR>";     t += "<TR>";   t += "  <TD class='right'><img src='http://cnt4.express.gr/xrimatistirio/images//go.gif' alt='close' id='"+make_name(this.name)+"_add_secsyms_go_ctl'  onclick=\""+this.name+".is_opened=true;"+this.name+".is_shown=false;"+this.name+".get();hide("+this.name+".inner_popup3);\" class='link' /></TD>";   t += "</TR>";        t += "</TABLE>";   return t;		 } function metoxes_moy_hide_afairesi_metoxis(e){   var is_clicked_inside = false;   var p = getElementPosition(this.inner_popup3);   var c = mouseCoords(e);   is_clicked_inside = ((c.x > p.left) && (c.x < p.left+p.width) && (c.y > p.top) && (c.y < p.top+p.height))?true:false;    if(!is_clicked_inside){     hide(this.inner_popup3)	     if(this.count>0 && p.width>0){       this.is_opened=true;       this.is_shown=false;       this.get();     }   }	   delete p;delete c;delete is_clicked_inside; } function metoxes_moy_show_help_afairesi_metoxis(that){   if(innerHTML(this.inner_help2,this.draw_help_afairesi_metoxis())){     var that_id = (that)?that.id:"";     var p = getElementPosition(that_id)     show(this.inner_help,(p.left),(p.top-53));     delete p;delete that_id;delete that;   } }  function metoxes_moy_draw_help_afairesi_metoxis(){   var t = "";   t += "<TABLE border='0' cellpadding='2' cellspacing='2' width='60'>";   t += "<TR>";   t += "  <TD class='white center'>Αφαίρεση Μετοχής</TD>";   t += "</TR>";   t += "</TABLE>";   return t; } function metoxes_moy_hide_help_afairesi_metoxis(){   hide(this.inner_help);	 } function metoxes_moy_rem(secsym, position){   if(typeof(this.set[secsym])=="object"){   	var tmp = new Array();   	for(var i in this.set){   	  if(i!=secsym){   	    tmp[i] = this.set[i];   	  }   	}   	this.set = tmp;   	innerHTML(this.inner_popup4,this.draw_afairesi_metoxis());   	delete tmp;   } }  function static_metoxes_moy_hide(e) {   e = e || window.event;	   for (var i in my_objects){     if (typeof(my_objects[i])=="object"){       if(my_objects[i].hide_prosthesi_metoxis) {         my_objects[i].hide_prosthesi_metoxis(e);       }       if(my_objects[i].hide_afairesi_metoxis){       	my_objects[i].hide_afairesi_metoxis(e);       }                 }   } } window.popupWindowOldEventListener2 = document.onmouseup; if (window.popupWindowOldEventListener2 != null) {   document.onmouseup = function(e){     window.popupWindowOldEventListener2(e);      static_metoxes_moy_hide(e);   } } else {   document.onmouseup = static_metoxes_moy_hide; } // // end class metoxes_moy // //--> </p>
