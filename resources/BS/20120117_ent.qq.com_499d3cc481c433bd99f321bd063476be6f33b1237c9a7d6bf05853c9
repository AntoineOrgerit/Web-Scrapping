<p> var SCRIPT_TIMEOUT = 20000; var QVPL_PATH = "http://mat1.gtimg.com/www/html5js/QVPL1.0.4.js"; function loadHelper (jsurl) { 	var oScriptEl, oTimeoutHDL, oHead; 	oScriptEl = document.createElement("script"); 	oScriptEl.type = "text/javascript"; 	oScriptEl.language = "javascript"; 	oScriptEl.src = jsurl; 	oScriptEl.onreadystatechange = doCallback;  	oScriptEl.onload = function() 	{ 		this.readyState = "complete"; 		doCallback();  		if(typeof(lianbo) == "object"){ 			lianbo.init(window.QVPL); 		} 	};  	oTimeoutHDL = window.setTimeout(doError,SCRIPT_TIMEOUT); 	document.getElementsByTagName("head")[0].appendChild(oScriptEl);  	function doCallback() 	{ 		if (oScriptEl.readyState == "complete" || oScriptEl.readyState == "loaded") 		{ 			oScriptEl.onload = oScriptEl.onreadystatechange = new Function(); 			window.clearTimeout(oTimeoutHDL); 		} 	};  	function doError() 	{ 		oScriptEl.parentNode.removeChild(oScriptEl); 	}; }  loadHelper(QVPL_PATH); </p>
<p> //20110713 if(document.addEventListener)         document.addEventListener("click", registerZone2, false); else if(document.attachEvent)         document.attachEvent("onclick", registerZone2); var gImage; //取自定义属性 function registerZone2(ev) {     var loopTryNum = 10;     var bossID = 451; 	var sep = "_"; 	var bossFilter = 6;     try 	{ 	    //qq 		var a=document.cookie.match(new RegExp('(^|)o_cookie=([^;]*)(;|$)')); 		var iQQ=(a==null?"":unescape(a[2])); 		var uin=parseInt(parseInt(iQQ)/100)%10;         if(uin!=bossFilter) return; 		 	    var ev = window.event || ev; 	    var et = ev.srcElement || ev.target; 		var type=et.tagName; 	    if (type != "A" && type != "IMG" )  		{ 		    return true; 		} 		 		//purl 		var purl=''; 		if (type == "A") 		{ 		    purl = et.href; 		} 		else if (type == "IMG") 		{ 		    purl = et.parentNode.href; 		} 		 		//pseudo attr 		var zoneId=""; 		for (var i=loopTryNum-1,tagNode=et;i>=0;i--,tagNode=tagNode.parentNode) 		{ 		    if(tagNode.attributes['bossZone']) 			{ 		        zoneId = tagNode.attributes['bossZone'].nodeValue; 			} 		    if(zoneId) break;  		}		 		if(!zoneId) return; 		 		//site 		var localUrl = location.href; 		var site = localUrl.substring(7,localUrl.indexOf('.qq.com')); 		 		var iurl = 'http://btrace.qq.com/collect?sIp=&iQQ='+iQQ+'&sBiz=qq.com&sOp='+zoneId+sep+site+'&iSta=&iTy='+bossID+'&iFlow=&sUrl='+escape(purl)+'&sLocalUrl='+escape(location.href)+'&'+Math.random(); 		gImage = new Image(1,1); 		gImage.src = iurl; 	} catch (e) {} } </p>
