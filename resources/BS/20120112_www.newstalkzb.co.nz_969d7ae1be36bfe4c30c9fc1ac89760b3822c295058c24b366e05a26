<p> function set_cookie(name, value, exdays, path, domain, secure) {         var cookie_string = name + "=" + escape(value);         if (exdays) {             var exdate = new Date();             exdate.setDate(exdate.getDate() + exdays);             cookie_string += "; expires=" + exdate.toUTCString();         }         if (path)             cookie_string += "; path=" + escape(path);         if (domain)             cookie_string += "; domain=" + escape(domain);         if (secure)             cookie_string += "; secure";         document.cookie = cookie_string;     }      function get_cookie(cookie_name) {         var results = document.cookie.match('(^|;) ?' + cookie_name + '=([^;]*)(;|$)');         if (results)             return (unescape(results[2]));         else         return "auckland";      }      var regions = [       { "region": 'Auckland', 'address': 'auckland' },       { "region": 'Christchurch', 'address': 'christchurch' },        { "region": 'Wellington', 'address': 'wellington' }  ];      function createRegionsList() {         for (var i = 0; i < regions.length; i++) {              //  $('#regions').append(new Option(regions[i].region, regions[i].address));             //for IE use this              $('#regions').append("<option value='" + regions[i].address + "' >" + regions[i].region + "</option>'");         }     }      function selectCurrentRegion(region) {         $('#regions').val(region);     }      function getRegions() {         var craddress = window.location.toString();         var newAddress;         newAddress = setRegionAddress(get_cookie("region"), craddress);         //console.log(newAddress);         window.location = newAddress;     }      function CheckCrAddress(region) {         var craddress = window.location.toString();         var reg = region;         craddress.toUpperCase();         reg.toUpperCase();         if (craddress.indexOf(reg) != -1) {             return true;         } else if ((craddress.indexOf(reg) == -1) && (region == "root")) {             return true;         }                  else {             return false;         }      }       function changeRegionCookie() {          set_cookie("region", $("#regions").val(), 1000, "/");         getRegions();     }      function setRegionAddress(region, currentaddress) {         var newaddress         var tempaddress;         tempaddress = currentaddress.toString();         tempaddress = tempaddress.replace("http://", "");         tempaddress = tempaddress.split("/");                  if (tempaddress[1] == undefined || tempaddress[1] == "") {                         if (region == "root") {                 newaddress = currentaddress             }             else {                 newaddress = currentaddress + region;             }         }         else if (checkRegExist(tempaddress[1])) {            // console.log("checkexisit" + tempaddress[1]);             if (region == "root") {                 // change to root from other regions                 var repstr =  tempaddress[1] + "/"                 newaddress = currentaddress.replace(repstr, "");             } else {                 // other region                   newaddress = currentaddress.replace(tempaddress[1], region);             }                       } else {            //console.log("else" + tempaddress[1]);             if (region == "root") {                 newaddress = currentaddress;             }             else {                 addreig = region + "/" + tempaddress[1];                 newaddress = currentaddress.replace(tempaddress[1], addreig);             }         }         return newaddress.toString();     }      function checkRegExist(region) {         var exist = false;         for (var i = 0; i < regions.length; i++) {             if (regions[i].address.toUpperCase() == region.toUpperCase()) {                 exist = true;             }          }         return exist;     }      function SetRegonPageLoad() {         createRegionsList();         selectCurrentRegion(get_cookie("region"));         if (!CheckCrAddress(get_cookie("region"))) {             //********* Do not Active this line until you have all pages for region *******************              getRegions();         }         $("#regions").change(changeRegionCookie); // getRegions            $("#regions").blur(changeRegionCookie);     }        SetRegonPageLoad(); </p>
