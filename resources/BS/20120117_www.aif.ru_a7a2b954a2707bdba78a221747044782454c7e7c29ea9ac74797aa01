eadonly="readonly" style="width: 457px; overflow:auto; border:1px solid #B4B4B4;background:transparent;">');             ta.height(text_block.height()+40);             buttons = $('<div class="reply_buttons"><a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_full">Цитировать полностью</a>  <a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_selected">Цитировать выделенное</a> <a href="javascript:void(0);" rel="#comment'+comment_id+'" class="reply_cancel">Отмена</a></div>');             ta.val($.trim(comment));               $('#comment'+ comment_id).append(buttons);             $('#comment'+ comment_id).append('<div id="reply_info_text">Для цитирования части комментария выделите нужную область текста и нажмите "Цитировать выделенное"</div>');             $('#comment'+ comment_id).append(ta);               $("a.reply_full").click(function()             {                 var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');                  var comment = $('blockquote', '#comment'+ comment_id).text();                  $('#comment_form_block').show();                 $('#show_comment_button').hide();                 reply(comment,comment_id);             });              $("a.reply_selected").click(function() {                 var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');                 text = selectedText($('#comment'+ comment_id + ' textarea'));                 if(text.length > 0) {                     $('#comment_form_block').show();                     $('#show_comment_button').hide();                     reply(text,comment_id);                 } else {                     aif.showError($(this),'Выделите текст для цитирования');                 }             });             $("a.reply_cancel").click(function()             {                 var comment_id = $(this).attr('rel').replace(/[^0-9]+/, '');                  $('div.reply_buttons', '#comment'+ comment_id).remove();                 $('textarea', '#comment'+ comment_id).remove();                 $('blockquote', '#comment'+ comment_id).show();                 $('#reply_info_text').remove();                 $('#reply_text').hide();             });         }     });      $("a.open").click(function() {         var li = $(this).parents("li.comment_closed");         li.attr("class","comment_opened");         cookie_id = cookie_prefix + li.find("div.comment-meta").attr("id");         $.cookie(cookie_id,'opened');     });     $("a.close").click(function() {         li = $(this).parents("li.comment_opened")         li.attr("class","comment_closed");         cookie_id = cookie_prefix + li.find("div.comment-meta").attr("id");         $.cookie(cookie_id,'closed');     });      $("a.complain").click(function() {         li = $(this).parents("li.comment_opened");         id = li.find("div.comment-meta").attr("id").replace('meta','');         var _self = this;          if($.cookie('comment'+id) != null) {             aif.showError($(_self),'Вы уже жаловались на данный комментарий!');             return;         }           $.ajax({             url: '/complaintadd/comment_id/' + id,             data: {},             type: 'POST',             cache: false,             success: function(response) {                 if(typeof response == "string" && response.charAt(0) == "{") response = jQuery.parseJSON(response);                 if(response.result) {                     /*if(response.content) {                         complaint_count = $(_self).parents("div.comment-meta").find("span.complaint_count");                         complaint_count.html(response.content);                     }*/                     aif.showError($(_self),response.result);                 }             },             error: function() {                 aif.showError($(_self),'Ошибка сервера');             }         });      });      $("a.full").click(function() {         //li = $(this).parents("li.comment_opened");         //li.find("span.comment-etc")