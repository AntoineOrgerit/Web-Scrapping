').html('Страница '+(curPage+1)+' из '+pages).css('margin-bottom', '10px'); 		else $('#pageofpage').html('').css('margin-bottom', '0'); 	} }  function changePage(pageN) { 	curPage = pageN-1; 	animate = true; 	rebuildContent(); }  function addLink() { 	var body_element = document.getElementsByTagName('body')[0];  	var pagelink = "<br><br> Подробности: <a href='"+location.href+"'>"+location.href+"</a><br>Любое использование материалов допускается только при наличии гиперссылки на 1news.az";  	var selection; 	if (Browser.Version() <= 9) 	{ 		selection = document.selection.createRange(); 		var copytext = selection.htmlText + pagelink; 	}else 	{ 		selection = window.getSelection(); 		var copytext = selection + pagelink; 	}  	var newdiv = document.createElement('div'); 	var newdiv2 = document.createElement('div');  	newdiv.style.position='absolute'; 	newdiv.style.left='-99999px'; 	newdiv.style.backgroundColor='white'; 	newdiv.style.color='black'; 	newdiv.style.textAlign='left'; 	newdiv.style.fontSize='12px';  	newdiv2.style.position='absolute'; 	newdiv2.style.left='-99999px';  	document.body.insertBefore(newdiv2, document.body.firstChild);  	newdiv.innerHTML= copytext;  	newdiv2.appendChild(document.createElement("br")); 	newdiv2.appendChild(newdiv);  	if (Browser.Version() <= 9) 	{ 		pagelink=document.body.createTextRange(); 		pagelink.moveToElementText(newdiv); 		pagelink.select(); 	}else 	{ 		selection.selectAllChildren(newdiv); 	}  //	setTimeout(function() {body_element.removeChild(newdiv);},0); }  var Browser = {   Version: function() {     var version = 999; // we assume a sane browser     if (navigator.appVersion.indexOf("MSIE") != -1)       // bah, IE again, lets downgrade version number       version = parseFloat(navigator.appVersion.split("MSIE")[1]);     return version;   } }  //--> </p>
